server {
	listen 4009;
	index index.html;

	location / {
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;

		proxy_redirect off;
		proxy_set_header Host 127.0.0.1;
		proxy_pass http://localhost:8009;
	}

	location /static/ {
		root /srv/www-data/group09/sec/;
	}
}
